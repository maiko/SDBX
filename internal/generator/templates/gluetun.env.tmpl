OPENVPN_PASSWORD={{if .Config.VPNPassword}}{{.Config.VPNPassword}}{{else}}your_password{{end}}

{{- else if eq .Config.VPNProvider "perfectprivacy"}}
# =============================================================================
# Perfect Privacy Configuration
# =============================================================================
OPENVPN_USER={{if .Config.VPNUsername}}{{.Config.VPNUsername}}{{else}}your_username{{end}}
OPENVPN_PASSWORD={{if .Config.VPNPassword}}{{.Config.VPNPassword}}{{else}}your_password{{end}}

{{- else if eq .Config.VPNProvider "airvpn"}}
# =============================================================================
# AirVPN Configuration
# Docs: https://airvpn.org/devices/
# =============================================================================
{{- if eq .Config.VPNType "wireguard"}}
WIREGUARD_PRIVATE_KEY={{if .Config.VPNWireguardKey}}{{.Config.VPNWireguardKey}}{{else}}your_private_key{{end}}
WIREGUARD_ADDRESSES={{if .Config.VPNWireguardAddr}}{{.Config.VPNWireguardAddr}}{{else}}your_address/32{{end}}
WIREGUARD_PRESHARED_KEY=
{{- else}}
OPENVPN_USER={{if .Config.VPNToken}}{{.Config.VPNToken}}{{else}}your_device_key{{end}}
{{- end}}

{{- else}}
# =============================================================================
# Custom VPN Configuration
# See: https://github.com/qdm12/gluetun-wiki/tree/main/setup/providers
# =============================================================================

# For OpenVPN custom config, mount your .ovpn file and set:
# VPN_TYPE=openvpn
# OPENVPN_CUSTOM_CONFIG=/gluetun/custom.ovpn
{{- if .Config.VPNUsername}}
OPENVPN_USER={{.Config.VPNUsername}}
{{- else}}
# OPENVPN_USER=your_username
{{- end}}
{{- if .Config.VPNPassword}}
OPENVPN_PASSWORD={{.Config.VPNPassword}}
{{- else}}
# OPENVPN_PASSWORD=your_password
{{- end}}

# For Wireguard custom:
# VPN_TYPE=wireguard
# WIREGUARD_PRIVATE_KEY=your_key
# WIREGUARD_ADDRESSES=10.x.x.x/32
# WIREGUARD_PUBLIC_KEY=peer_public_key
# VPN_ENDPOINT_IP=vpn_server_ip
# VPN_ENDPOINT_PORT=51820

{{- end}}

# =============================================================================
# Advanced Options
# =============================================================================

# DNS over TLS
DOT=on
DOT_PROVIDERS=cloudflare

# Firewall kill-switch (enabled by default)
# FIREWALL=on

# Allow LAN access
# FIREWALL_OUTBOUND_SUBNETS=192.168.1.0/24

# Health check (for container orchestration)
# HEALTH_VPN_DURATION_INITIAL=10s
# HEALTH_VPN_DURATION_ADDITION=5s
