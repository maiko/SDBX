# =============================================================================
# Gluetun VPN Configuration
# Generated by sdbx init
#
# Documentation: https://github.com/qdm12/gluetun-wiki
# Provider docs: https://github.com/qdm12/gluetun-wiki/tree/main/setup/providers
# =============================================================================

# Provider and protocol
VPN_SERVICE_PROVIDER={{.Config.VPNProvider}}
VPN_TYPE={{.Config.VPNType}}

# Server selection
{{- if .Config.VPNCountry}}
SERVER_COUNTRIES={{.Config.VPNCountry}}
{{- else}}
# SERVER_COUNTRIES=Netherlands
{{- end}}
# SERVER_REGIONS=
# SERVER_CITIES=
# SERVER_HOSTNAMES=

{{- if eq .Config.VPNProvider "nordvpn"}}
# =============================================================================
# NordVPN Configuration
# Docs: https://my.nordaccount.com/dashboard/nordvpn/manual-configuration/
# =============================================================================
{{- if eq .Config.VPNType "wireguard"}}
WIREGUARD_PRIVATE_KEY={{if .Config.VPNWireguardKey}}{{.Config.VPNWireguardKey}}{{else}}your_wireguard_private_key{{end}}
{{- else}}
OPENVPN_USER={{if .Config.VPNUsername}}{{.Config.VPNUsername}}{{else}}your_service_username{{end}}
OPENVPN_PASSWORD={{if .Config.VPNPassword}}{{.Config.VPNPassword}}{{else}}your_service_password{{end}}
{{- end}}
SERVER_CATEGORIES=P2P

{{- else if eq .Config.VPNProvider "mullvad"}}
# =============================================================================
# Mullvad Configuration
# Docs: https://mullvad.net/en/account/
# =============================================================================
{{- if eq .Config.VPNType "wireguard"}}
WIREGUARD_PRIVATE_KEY={{if .Config.VPNWireguardKey}}{{.Config.VPNWireguardKey}}{{else}}your_private_key{{end}}
WIREGUARD_ADDRESSES={{if .Config.VPNWireguardAddr}}{{.Config.VPNWireguardAddr}}{{else}}your_address/32{{end}}
{{- else}}
OPENVPN_USER={{if .Config.VPNToken}}{{.Config.VPNToken}}{{else}}your_account_number{{end}}
{{- end}}

{{- else if eq .Config.VPNProvider "protonvpn"}}
# =============================================================================
# ProtonVPN Configuration
# Docs: https://account.protonvpn.com/account#openvpn
# =============================================================================
OPENVPN_USER={{if .Config.VPNUsername}}{{.Config.VPNUsername}}{{else}}your_openvpn_username{{end}}
OPENVPN_PASSWORD={{if .Config.VPNPassword}}{{.Config.VPNPassword}}{{else}}your_openvpn_password{{end}}

{{- else if eq .Config.VPNProvider "pia"}}
# =============================================================================
# Private Internet Access Configuration
# =============================================================================
OPENVPN_USER={{if .Config.VPNUsername}}{{.Config.VPNUsername}}{{else}}your_username{{end}}
OPENVPN_PASSWORD={{if .Config.VPNPassword}}{{.Config.VPNPassword}}{{else}}your_password{{end}}

{{- else if eq .Config.VPNProvider "surfshark"}}
# =============================================================================
# Surfshark Configuration
# Docs: https://my.surfshark.com/vpn/manual-setup/main
# =============================================================================
OPENVPN_USER={{if .Config.VPNUsername}}{{.Config.VPNUsername}}{{else}}your_username{{end}}
OPENVPN_PASSWORD={{if .Config.VPNPassword}}{{.Config.VPNPassword}}{{else}}your_password{{end}}

{{- else if eq .Config.VPNProvider "expressvpn"}}
# =============================================================================
# ExpressVPN Configuration
# Docs: https://www.expressvpn.com/setup#manual
# =============================================================================
OPENVPN_USER={{if .Config.VPNUsername}}{{.Config.VPNUsername}}{{else}}your_username{{end}}
OPENVPN_PASSWORD={{if .Config.VPNPassword}}{{.Config.VPNPassword}}{{else}}your_password{{end}}

{{- else if eq .Config.VPNProvider "windscribe"}}
# =============================================================================
# Windscribe Configuration
# Docs: https://windscribe.com/getconfig/openvpn
# =============================================================================
OPENVPN_USER={{if .Config.VPNUsername}}{{.Config.VPNUsername}}{{else}}your_username{{end}}
OPENVPN_PASSWORD={{if .Config.VPNPassword}}{{.Config.VPNPassword}}{{else}}your_password{{end}}

{{- else if eq .Config.VPNProvider "ipvanish"}}
