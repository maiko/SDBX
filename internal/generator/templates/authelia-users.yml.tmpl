# Authelia Users Database
# Generated by sdbx init
#
# To generate a password hash, run:
#   docker run --rm authelia/authelia:latest authelia crypto hash generate argon2
#
# Default user: admin / admin (CHANGE THIS!)

users:
  {{.Config.AdminUser}}:
    displayname: "Admin"
    # Password set during init (Argon2id hash)
    password: "{{.Config.AdminPasswordHash}}"
    email: {{.Config.AdminUser}}@{{.Config.Domain}}
    groups:
      - admins
      - users
